{{!-- The user tabs. Tabs can be added and deleted --}}
{{user-tabs deleteTab=(action "deleteTab") setTab=(action "changeTab") addTab=(action "addTab") usernames=model.usernames showInputBox=showInputBox}}

{{!-- The list of flows of the user --}}
{{#if loading}}
  {{loading-panel message="loading flows..."}}
{{else}}
  {{flow-list workflows=model.workflows}}
{{/if}}
